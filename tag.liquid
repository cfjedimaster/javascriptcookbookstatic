---
pagination:
    data: collections.tagList
    size: 1
    alias: tag
permalink: tag/{{ tag | slug }}/index.html
layout: default
---

<h3>Tag: {{tag}}</h3>

<ul>
{% for article in collections[tag] %}
    <li><a href="{{article.url}}">{{ article.data.title }}</a></li>
{% endfor %}
</ul>
